var
  G_TotalCount: Integer = 0;
  G_Price: Integer= 79900;
  G_UnprocessCount: Integer= 100;

{
function GetTestData: string;
var
  LString: string;
  LStrings: TStringList;
  LSecond: Integer;
begin
  LString := '20150627112941,B,2015年6月27日上海市个人非营业性客车额度投标拍卖会,7441,172205,10:30,11:30,10:30,11:00,11:00,11:30,11:29:40,79100,2015-06-27 11:29:36,78800,79400,504748,0';
  LStrings := TStringList.Create;
  LStrings.CommaText := LString;
  LSecond := SecondOf(Now);
  G_Price := G_Price + 100;
  G_UnprocessCount := G_UnprocessCount + 30;
  if LSecond mod 3 = 0 then
  begin
    G_TotalCount := G_TotalCount + 3500;
  end
  else if LSecond mod 3 = 1 then
  begin
    G_TotalCount := G_TotalCount + 3200;
  end
  else if LSecond mod 3 = 2 then
  begin
    G_TotalCount := G_TotalCount + 3200;
  end;
  LStrings[18] := IntToStr(G_UnprocessCount);
  LStrings[17] := IntToStr(G_TotalCount);
  LStrings[12] := IntToStr(G_Price);

  Result := LStrings.CommaText;
  LStrings.Free;
end;
}

